regra de produção

valor : ARRAY
      | OBJETO <-- não terminais ->extensível
      | "true" <--- terminais ->
      | "false"
      | ...
ARRAY : "[" "]"
      | "[" valor ("," valor)* "]"


EBNF (extende c/ operadores do tipo *, + , ?)

ARRAY : "[" "]"
      | "[" ITENS "]"
ITENS : VALOR
      | VALOR "," ITENS


